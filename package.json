{
  "name": "stratosort",
  "version": "1.0.0",
  "description": "Stratosort - Local AI Document Organizer with Ollama integration. Privacy-focused document organization using AI-powered analysis.",
  "keywords": [
    "electron",
    "ai",
    "document-organization",
    "ollama",
    "local-ai",
    "privacy",
    "file-organizer",
    "stratosort",
    "fibonacci-ui",
    "document-analysis"
  ],
  "homepage": "https://github.com/stratosort/stratosort#readme",
  "bugs": {
    "url": "https://github.com/stratosort/stratosort/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stratosort/stratosort.git"
  },
  "license": "MIT",
  "author": {
    "name": "Stratosort Team",
    "email": "contact@stratosort.com",
    "url": "https://stratosort.com"
  },
  "contributors": [
    {
      "name": "Amila Welihinda",
      "email": "amilajack@gmail.com",
      "url": "https://github.com/amilajack"
    }
  ],
  "main": "./src/main/simple-main.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "npm run build:dev && cross-env NODE_ENV=development electron . --enable-logging",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:nsis": "node scripts/generate-nsis-assets.js",
    "generate:assets": "npm run generate:icons && npm run generate:nsis",
    "postinstall": "electron-builder install-app-deps && node setup-ollama.js --auto --ci-skip",
    "setup:ollama": "node setup-ollama.js",
    "setup:ollama:check": "node setup-ollama.js --check",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx",
    "lint:fix": "cross-env NODE_ENV=development eslint . --ext .js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "package": "rimraf dist && npm run build && electron-builder build --publish never",
    "start": "npm run build:dev && cross-env NODE_ENV=development electron . --enable-logging",
    "start:prod": "cross-env NODE_ENV=production electron .",
    "start:debug": "cross-env NODE_ENV=development electron . --enable-logging --inspect",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "ollama:check": "node setup-ollama.js --check",
    "ollama:models": "ollama pull gemma3:4b",
    "dist": "npm run build && electron-builder --publish never --config electron-builder.json",
    "dist:win": "npm run build && electron-builder --win --publish never --config electron-builder.json",
    "dist:mac": "npm run build && electron-builder --mac --publish never --config electron-builder.json",
    "dist:linux": "npm run build && electron-builder --linux --publish never --config electron-builder.json",
    "dist:all": "npm run build && electron-builder --mac --win --linux --publish never --config electron-builder.json"
  },
  "browserslist": [],
  "overrides": {
    "@jridgewell/gen-mapping": "^0.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "projects": [
      "<rootDir>/test/jest.config.js"
    ]
  },
