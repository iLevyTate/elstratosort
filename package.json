{
  "name": "stratosort",
  "version": "1.0.0",
  "description": "Stratosort - Local AI Document Organizer with Ollama integration. Privacy-focused document organization using AI-powered analysis.",
  "keywords": [
    "electron",
    "ai",
    "document-organization",
    "ollama",
    "local-ai",
    "privacy",
    "file-organizer",
    "elstratosort",
    "document-analysis"
  ],
  "homepage": "https://github.com/stratosort/stratosort#readme",
  "bugs": {
    "url": "https://github.com/stratosort/stratosort/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stratosort/stratosort.git"
  },
  "license": "MIT",
  "author": {
    "name": "Stratosort Team",
    "url": "https://github.com/stratosort/stratosort"
  },
  "contributors": [
    {
      "name": "Amila Welihinda",
      "email": "amilajack@gmail.com",
      "url": "https://github.com/amilajack"
    }
  ],
  "main": "./dist/main.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "npm run build:dev && npm run build:main:dev && cross-env NODE_ENV=development USE_DEV_SERVER=true electron . --enable-logging",
    "build": "rimraf dist && npm run build:main && cross-env NODE_ENV=production webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:main": "webpack --config webpack.main.config.js --mode=production",
    "build:main:dev": "webpack --config webpack.main.config.js --mode=development",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:nsis": "node scripts/generate-nsis-assets.js",
    "generate:channels": "node scripts/generate-preload-channels.js",
    "generate:channels:check": "node scripts/generate-preload-channels.js --check",
    "generate:channels:update": "node scripts/generate-preload-channels.js --update",
    "generate:assets": "npm run generate:icons && npm run generate:nsis",
    "postinstall": "node scripts/postinstall.js",
    "setup:ollama": "node scripts/setup-ollama.js",
    "setup:ollama:check": "node scripts/setup-ollama.js --check",
    "setup:chromadb": "node scripts/setup-chromadb.js --auto",
    "setup:chromadb:check": "node scripts/setup-chromadb.js --check",
    "setup:deps": "node scripts/setup-ollama.js --auto && node scripts/setup-chromadb.js --auto",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"test/**/*.{js,ts}\" \"scripts/**/*.js\" \"*.{js,json}\" \"webpack.config.js\" \"babel.config.js\" \"postcss.config.js\" \"playwright.config.js\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" \"test/**/*.{js,ts}\" \"scripts/**/*.js\" \"*.{js,json}\" \"webpack.config.js\" \"babel.config.js\" \"postcss.config.js\" \"playwright.config.js\"",
    "prepare": "husky install",
    "package": "rimraf dist && npm run build && electron-builder build --publish never",
    "start": "npm run build:dev && cross-env NODE_ENV=development electron . --enable-logging",
    "start:prod": "cross-env NODE_ENV=production electron .",
    "start:debug": "cross-env NODE_ENV=development electron . --enable-logging --inspect",
    "test": "jest --config test/jest.config.js",
    "test:coverage": "jest --config test/jest.config.js --coverage",
    "test:watch": "jest --config test/jest.config.js --watch",
    "test:perf": "jest --config test/performance/jest.config.js --runInBand",
    "test:stress": "jest --config test/stress/jest.config.js --runInBand",
    "test:integration": "jest test/integration --runInBand",
    "test:all": "npm run test && npm run test:perf && npm run test:stress",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:report": "npx playwright show-report test-results/e2e-report",
    "ollama:check": "node scripts/setup-ollama.js --check",
    "ollama:models": "ollama pull gemma3:4b",
    "dist": "npm run build && electron-builder --publish never --config electron-builder.json",
    "dist:win": "npm run build && electron-builder --win --publish never --config electron-builder.json",
    "dist:mac": "npm run build && electron-builder --mac --publish never --config electron-builder.json",
    "dist:linux": "npm run build && electron-builder --linux --publish never --config electron-builder.json",
    "dist:all": "npm run build && electron-builder --mac --win --linux --publish never --config electron-builder.json",
    "ci": "npm run format:check && npm run lint && npm test && npm run build"
  },
  "browserslist": [],
  "overrides": {
    "@jridgewell/gen-mapping": "^0.3.5",
    "js-yaml": "^4.1.1"
  },
  "jest": {
    "projects": [
      "<rootDir>/test/jest.config.js"
    ]
  },
  "dependencies": {
    "@orama/orama": "^3.1.18",
    "@xenova/transformers": "^2.17.2",
    "chokidar": "^5.0.0",
    "chromadb": "^3.2.0",
    "electron-updater": "^6.7.3",
    "electron-window-state": "^5.0.3",
    "elkjs": "^0.11.0",
    "fast-xml-parser": "^5.3.3",
    "framer-motion": "^12.24.7",
    "lucide-react": "^0.562.0",
    "lunr": "^2.3.9",
    "mammoth": "^1.11.0",
    "node-fetch": "^3.3.2",
    "node-tesseract-ocr": "^2.2.1",
    "officeparser": "^6.0.1",
    "ollama": "^0.6.3",
    "pdf-parse": "^2.4.5",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-redux": "^9.2.0",
    "react-window": "^2.2.4",
    "reactflow": "^11.11.4",
    "sharp": "^0.34.5",
    "xlsx-populate": "^1.21.0"
  },
