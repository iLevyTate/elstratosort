    "axios": "^1.13.2",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "browserslist-config-erb": "^0.0.3",
    "buffer": "^6.0.3",
    "chalk": "^5.6.2",
    "concurrently": "^9.2.1",
    "core-js": "^3.47.0",
    "cross-env": "^10.1.0",
